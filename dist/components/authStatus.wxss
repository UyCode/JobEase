.submintSuccess {
  display: flex;
  padding: 32rpx;
  flex-direction: column;
  align-items: center;
  z-index: 100
}
.submintSuccess .icon::before {
  content: '\5bf9';
  font-family: 'TieJia';
  color: #00ff66;
  font-size: 100rpx
}
.submintSuccess .status {
  display: inline-block;
  padding: 54rpx 0 24rpx 0;
  font-size: 34rpx;
  color: #2d2d33;
  letter-spacing: .3rpx;
  text-align: center;
  font-weight: bold
}
.submintSuccess .modifyInfo {
  margin-top: 90rpx;
  padding: 20rpx 100rpx;
  background: rgba(59, 106, 251, 0.1);
  border-radius: 4rpx;
  font-size: 34rpx;
  color: #3b6afb;
  letter-spacing: .3rpx;
  text-align: center
}
