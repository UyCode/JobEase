.container {
  width: 100%;
  height: 100%;
  overflow: hidden
}
.container .poup {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 999
}
.container .poup .poupContent {
  position: relative;
  width: 594rpx;
  box-sizing: border-box;
  padding: 44rpx 40rpx;
  background: #ffffff;
  border-radius: 4rpx;
  background-color: #ffffff
}
.container .poup .poupContent .icon {
  position: absolute;
  right: 22rpx;
  top: 22rpx
}
.container .poup .poupContent .icon::after {
  content: '\e626';
  display: block;
  font-family: 'TieJia';
  font-size: 22rpx;
  vertical-align: middle
}
.container .poup .poupContent .title {
  margin-top: 40rpx;
  font-size: 30rpx;
  color: #2d2d33;
  letter-spacing: .4rpx;
  text-align: center;
  line-height: 49rpx
}
.container .poup .poupContent .liAuth {
  background: #3b6afb;
  width: 100%;
  height: 80rpx;
  margin: 68rpx 0 55rpx 0;
  line-height: 80rpx;
  border-radius: 4rpx;
  font-size: 30rpx;
  color: #ffffff;
  letter-spacing: .4rpx;
  text-align: center
}
.container .poup .poupContent .awaitAuth {
  background: #f5f8fa;
  border-radius: 4rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  color: #4a4a53;
  letter-spacing: .37rpx;
  text-align: center
}
.container .mapHeader {
  width: 100%;
  height: 250rpx;
  background-color: #ffffff
}
.container .mapHeader .tarBars {
  position: relative;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center
}
.container .mapHeader .tarBars .icon {
  position: absolute;
  right: 0;
  display: flex;
  flex-direction: row;
  align-items: center
}
.container .mapHeader .tarBars .icon::after {
  content: '\95ee';
  font-family: 'TieJia';
  position: absolute;
  display: inline-block;
  height: 68rpx;
  line-height: 68rpx;
  font-size: 34rpx;
  color: #6a6a77;
  right: 30rpx
}
.container .mapHeader .tarBars .tabItem .tabBtn {
  width: 250rpx;
  height: 68rpx;
  line-height: 68rpx;
  background: #ffffff;
  border: 1rpx solid #dce2ee;
  font-size: 32rpx;
  color: #6a6a77;
  letter-spacing: .28rpx;
  text-align: center
}
.container .mapHeader .tarBars .tabItem .tabBtn.active {
  background: #3b6afb;
  color: #ffffff;
  border-radius: 4rpx 0 0 4rpx;
  border: none
}
.container .mapHeader .screen {
  padding: 10rpx 0 20rpx 0
}
.container .mapHeader .screen .screen-content {
  display: flex;
  padding: 20rpx 0;
  flex-direction: row;
  justify-content: center;
  align-items: center
}
.container .mapHeader .screen .screen-content .screen-item {
  position: relative;
  flex: 1;
  height: 40rpx;
  line-height: 40rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 28rpx;
  color: #6a6a77;
  letter-spacing: .37rpx;
  text-align: center;
  justify-content: center
}
.container .mapHeader .screen .screen-content .screen-item.screen-item-active {
  color: #3b6afb
}
.container .mapHeader .screen .screen-content .screen-item.screen-item-active .text::after {
  display: inline-block;
  margin-left: 8rpx;
  content: '\e665';
  font-family: 'TieJia';
  color: #3b6afb;
  font-size: 14rpx
}
.container .mapHeader .screen .screen-content .screen-item .text {
  display: flex;
  flex: 1;
  height: 40rpx;
  line-height: 40rpx;
  flex-direction: row;
  align-items: center;
  justify-content: center
}
.container .mapHeader .screen .screen-content .screen-item .text::after {
  display: block;
  margin-left: 8rpx;
  content: '\e665';
  font-family: 'TieJia';
  color: #bdc1c9;
  font-size: 14rpx
}
.container .mapHeader .screen .screen-content .screen-item .text text {
  display: block;
  max-width: 150rpx;
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap
}
.container .mapHeader .screen .screen-content .screen-item.active::before {
  position: absolute;
  left: -15rpx;
  content: '\e63a';
  font-family: 'TieJia';
  color: #dce2ee;
  font-size: 28rpx
}
.container .mapHeader .sreenLabel {
  width: 100%;
  box-sizing: border-box;
  overflow: hidden
}
.container .mapHeader .sreenLabel .content {
  position: relative
}
.container .mapHeader .sreenLabel .content .labelContent {
  white-space: nowrap
}
.container .mapHeader .sreenLabel .content .labelContent :nth-child(1) {
  margin-left: 20rpx
}
.container .mapHeader .sreenLabel .content .labelContent .item {
  display: inline-block;
  margin-right: 20rpx;
  padding: 0 20rpx;
  height: 56rpx;
  line-height: 56rpx;
  font-size: 26rpx;
  color: #4a4a53;
  letter-spacing: .3rpx;
  text-align: center;
  background: #f5f8fa;
  border-radius: 4rpx
}
.container .mapHeader .sreenLabel .content .labelContent .item.active {
  background: #3b6afb;
  color: #ffffff;
  font-weight: 300
}